# only process documented members in our main header
EXTRACT_ALL=no
INPUT=libtelnet.h

# libtelnet is all C
OPTIMIZE_OUTPUT_FOR_C=yes

# put all output into the doc directory
OUTPUT_DIRECTORY=doc

# generate HTML and man pages only
GENERATE_LATEX=no
GENERATE_RTF=no
GENERATE_MAN=yes
GENERATE_HTML=yes
